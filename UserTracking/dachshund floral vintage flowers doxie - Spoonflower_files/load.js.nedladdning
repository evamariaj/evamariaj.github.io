    }
  },

  unset: function (name) {

    exitintel.cookie.set(name, '', -10);
  },

  nuke: function (name) {

    name = exitintel.cookie.prefix + name;

    document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;path=/;';
  },

  getJSON: function (name, defaultValue) {

    var strValue = exitintel.cookie.get(name);

    var val;
