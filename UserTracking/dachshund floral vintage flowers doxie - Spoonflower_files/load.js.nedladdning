    } catch (e) {
      val = null;
    }

    if (!val && defaultValue) {
      return defaultValue;
    }

    return val;
  },

  setJSON: function (name, value, days) {
    var strValue = JSON.stringify(value);

    exitintel.cookie.set(name, strValue, days);
  },

  resetAll: function () {
    var cookies = document.cookie.split(';').map(function (c) {
      var parts = c.split('=');
      return {
        name: parts[0].trim(),
        value: parts[1]
      };
    });

    var exiCookies = cookies.filter(function (c) {
